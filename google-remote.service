[Unit]
Description=Google Remote
After=multi-user.target

[Service]
ExecStart=/bin/bash -c '/home/pi/projects/google-remote/run.sh > /home/pi/google-remote.log 2>&1'
Restart=always
RestartSec=10s
KillMode=process
TimeoutSec=infinity
TTYPath=/dev/tty1
StandardInput=tty

[Install]
WantedBy=multi-user.target
